{
  "name": "mdast-util-heading-range",
  "version": "1.0.2",
  "description": "Markdown heading as ranges in mdast",
  "license": "MIT",
  "keywords": [
    "markdown",
    "heading",
    "range",
    "mdast"
  ],
  "dependencies": {
    "mdast-util-to-string": "^1.0.0"
  },
  "repository": "wooorm/mdast-util-heading-range",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^3.0.0",
    "remark-lint": "^2.0.0",
    "remark-toc": "^2.0.0",
    "remark-usage": "^2.0.0",
    "remark-validate-links": "^2.0.0",
    "mocha": "^2.0.0"
  },
  "scripts": {
    "test-api": "mocha --check-leaks test.js",
    "test-coverage": "istanbul cover _mocha -- test.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "bundle": "browserify index.js --no-builtins -s mdastUtilHeadingRange > mdast-util-heading-range.js",
    "postbundle": "esmangle mdast-util-heading-range.js > mdast-util-heading-range.min.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run bundle && npm run build-md"
  }
}
